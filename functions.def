function GetRealValue {
	did=$1
	val=$2
	sid=${did:0:3}
	if [ "$sid" == "28." ]
	then

		erg=$( echo "$val > 70" | bc)
		if [ $erg == "1" ]
		then
			nv=$(echo "$val""*3.2+700" | bc)
			FinalRound $nv
		else
			FinalRound $val
		fi


	else
		FinalRound $val
	fi

}
